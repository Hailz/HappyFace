<%= console.log(result.name, "____________", result)%>

<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
  <div class="result box">
    <a href="<%= result.product_link %>">
     <img class="resultImg img-thumbnail" src="<%= result.image_link %>">
     <h3 class="productName"><%= result.name %></h3>
     <% if (result.tag_list.length > 0) {%>
      <p>Product tags: <%= result.tag_list %></p>
     <% } else{ %>
      <p>No tags are available for this product.</p>
     <% } %>
    </a>

    <% if(currentUser){ %>
      <form method="POST" action="/vanity">
        <input hidden type="text" name="name" value="<%= result.name %>">
        <input hidden type="text" name="productType" value=" <%= result.product_type %>">
        <input hidden type="text" name="imageLink" value=" <%= result.image_link %>">
        <input hidden type="text" name="productLink" value=" <%= result.product_link %>">
        <button class="btn btnColor" type="submit">Add to Vanity</button>
      </form>
    <% } %>
  </div>
</div>