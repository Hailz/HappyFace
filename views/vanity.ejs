<h1>My Vanity</h1>

<div>
  <% products.forEach(function(product){ %>
    <a href="<%= product.product_link %>">
    <img src="<%= product.image_link %>">
    </a>
    <h3><%= product.name %></h3>
  <% }) %>
</div>

